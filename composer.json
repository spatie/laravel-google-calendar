{
    "name": "spatie/laravel-google-calendar",
    "description": "Manage events on a Google Calendar",
    "keywords": [
        "spatie",
        "calendar",
        "google",
        "event",
        "schedule",
        "laravel-google-calendar"
    ],
    "homepage": "https://github.com/spatie/laravel-google-calendar",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.0",
        "google/apiclient": "^1.1.7",
        "illuminate/support": "^5.2.8",
        "nesbot/carbon": "^1.21"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.3.4",
        "orchestra/testbench": "^3.2",
        "mockery/mockery": "^0.9.5"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\GoogleCalendar\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\GoogleCalendar\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
